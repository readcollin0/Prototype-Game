            <kind>1</kind>
            <string>if (leave) {
    if (waitFrames &gt; 0) {
        waitFrames--;
    } else {
        room_goto(ROOM_START);
        global.player.visible = true;
        if (global.transitionPlayer) {
            global.player.x = global.nextRoomX;
            global.player.y = global.nextRoomY;
            global.player = global.nextPlayerDir;
        }
    }
} else {
    var enter = keyboard_check_pressed(vk_enter);
    var space = keyboard_check_pressed(vk_space);
    if (enter || space) {
        fadeIn = false;
        waitFrames = 0;
        if (keyboard_check_pressed(vk_enter)) {
            skip = true;
        }
    }
    
    if (fadeIn) {
        alpha += TEXT_FADE_RATE;
        if (alpha &gt;= 1) {
            alpha = 1;
            fadeIn = false;
            waitFrames = text[currentMessage,1] * room_speed;
        }   
    } else if (waitFrames &gt; 0) {
        waitFrames--;
    } else if (alpha &gt; 0) {
        alpha -= TEXT_FADE_RATE;
        if (alpha &lt;= 0) {
            alpha = 0;
            waitFrames = TEXT_TIME_SEP;
        }
    } else {
        fadeIn = true;
        waitFrames = WAIT_TIME;
        alpha = 0;
        currentMessage++;
        if (currentMessage == array_height_2d(text) || skip) {
            leave = true;
            waitFrames = WAIT_TIME;
            fade(FADE_RATE, WAIT_TIME, FADE_RATE);
        }
    }
    if (!leave) draw_text_colour(TEXT_X, TEXT_Y, text[currentMessage,0], c_white, c_white, c_white, c_white, alpha);
}
